@model IEnumerable<HabitAqui.Data.Arrendamento>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.DataInicioContrato)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DataFimContrato)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CustoArrendamento)
            </th>
            
            <th>
                @Html.DisplayNameFor(model => model.estado)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.AspNetUser)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.DataInicioContrato)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DataFimContrato)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CustoArrendamento)
            </td>
            
            <td>
                @Html.DisplayFor(modelItem => item.estado)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AspNetUser.Id)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
